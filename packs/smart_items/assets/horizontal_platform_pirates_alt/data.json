{
  "id": "d45c2c0e-56b0-433f-8a0b-e80184f25dbd",
  "name": "Horizontal Platform Pirates Alt",
  "category": "structures",
  "tags": [
    "platform",
    "horizontal",
    "interactive"
  ],
  "components": {
    "core::GltfContainer": {
      "src": "{assetPath}/horizontal_platform_pirates_alt.glb",
      "visibleMeshesCollisionMask": 2,
      "invisibleMeshesCollisionMask": 0
    },
    "asset-packs::Actions": {
      "id": "{self}",
      "value": [
        {
          "name": "Going to Start",
          "type": "set_state",
          "jsonPayload": "{ \"state\": \"GoingToStart\" }"
        },
        {
          "name": "Going to End",
          "type": "set_state",
          "jsonPayload": "{ \"state\": \"GoingToEnd\" }"
        },
        {
          "name": "Go to End",
          "type": "start_tween",
          "jsonPayload": "{ \"type\": \"move_item\", \"end\": { \"x\": 10, \"y\": 0, \"z\": 0 }, \"relative\": true, \"interpolationType\": \"linear\", \"duration\": 5 }"
        },
        {
          "name": "Go to Start",
          "type": "start_tween",
          "jsonPayload": "{ \"type\": \"move_item\", \"end\": { \"x\": -10, \"y\": 0, \"z\": 0 }, \"relative\": true, \"interpolationType\": \"linear\", \"duration\": 5 }"
        }
      ]
    },
    "asset-packs::Triggers": {
      "value": [
        {
          "type": "on_state_change",
          "conditions": [
            {
              "id": "{self:asset-packs::States}",
              "type": "when_state_is",
              "value": "GoingToStart"
            }
          ],
          "actions": [
            {
              "id": "{self:asset-packs::Actions}",
              "name": "Go to Start"
            }
          ]
        },
        {
          "type": "on_state_change",
          "conditions": [
            {
              "id": "{self:asset-packs::States}",
              "type": "when_state_is",
              "value": "GoingToEnd"
            }
          ],
          "actions": [
            {
              "id": "{self:asset-packs::Actions}",
              "name": "Go to End"
            }
          ]
        },
        {
          "type": "on_tween_end",
          "conditions": [
            {
              "id": "{self:asset-packs::States}",
              "type": "when_state_is",
              "value": "GoingToEnd"
            }
          ],
          "actions": [
            {
              "id": "{self:asset-packs::Actions}",
              "name": "Going to Start"
            }
          ]
        },
        {
          "type": "on_tween_end",
          "conditions": [
            {
              "id": "{self:asset-packs::States}",
              "type": "when_state_is",
              "value": "GoingToStart"
            }
          ],
          "actions": [
            {
              "id": "{self:asset-packs::Actions}",
              "name": "Going to End"
            }
          ]
        },
        {
          "type": "on_spawn",
          "actions": [
            {
              "id": "{self:asset-packs::Actions}",
              "name": "Go to End"
            }
          ]
        }
      ]
    },
    "asset-packs::States": {
      "id": "{self}",
      "value": [
        "GoingToEnd",
        "GoingToStart"
      ],
      "defaultValue": "GoingToEnd"
    }
  }
}