{
  "id": "669b910d-c45c-4712-baa8-7e7e9fde3db2",
  "name": "Vertical Purple Pad",
  "category": "platforms",
  "tags": ["platform", "vertical", "interactive"],
  "components": {
    "core::GltfContainer": {
      "src": "{assetPath}/vertical_purple_pad.glb",
      "visibleMeshesCollisionMask": 2,
      "invisibleMeshesCollisionMask": 2
    },
    "asset-packs::Actions": {
      "id": "{self}",
      "value": [
        {
          "name": "Go to Start",
          "type": "set_state",
          "jsonPayload": "{ \"state\": \"GoingToStart\" }"
        },
        {
          "name": "Go to End",
          "type": "set_state",
          "jsonPayload": "{ \"state\": \"GoingToEnd\" }"
        }
      ]
    },
    "asset-packs::Triggers": {
      "value": [
        {
          "type": "on_tween_end",
          "conditions": [
            {
              "id": "{self:asset-packs::States}",
              "type": "when_state_is",
              "value": "GoingToEnd"
            }
          ],
          "actions": [
            {
              "id": "{self:asset-packs::Actions}",
              "name": "Go to Start"
            }
          ],
          "basicViewId": "trigger-when-end-reached"
        },
        {
          "type": "on_tween_end",
          "conditions": [
            {
              "id": "{self:asset-packs::States}",
              "type": "when_state_is",
              "value": "GoingToStart"
            }
          ],
          "actions": [
            {
              "id": "{self:asset-packs::Actions}",
              "name": "Go to End"
            }
          ],
          "basicViewId": "trigger-when-start-reached"
        }
      ]
    },
    "asset-packs::States": {
      "id": "{self}",
      "value": ["GoingToEnd", "GoingToStart"],
      "defaultValue": "GoingToEnd"
    },
    "core-schema::Sync-Components": {
      "value": ["core::Tween", "asset-packs::States"]
    },
    "core::Tween": {
      "duration": 5000,
      "easingFunction": 0,
      "mode": {
        "$case": "move",
        "move": {}
      },
      "playing": true
    },
    "core::TweenSequence": {
      "sequence": [],
      "loop": 1
    },
    "inspector::Config": {
      "isBasicViewEnabled": true,
      "componentName": "Vertical Platform",
      "fields": [
        {
          "name": "Tween",
          "type": "core::Tween"
        },
        {
          "name": "When End Reached",
          "type": "asset-packs::Triggers",
          "basicViewId": "trigger-when-end-reached"
        },
        {
          "name": "When Start Reached",
          "type": "asset-packs::Triggers",
          "basicViewId": "trigger-when-start-reached"
        }
      ]
    }
  }
}
